import javax.swing.JFrame;

/**
 * Main class that sets up the JFrame
 * @author anuccio
 *
 */
public class KeyboardTester {


	public static void main(String[] args) {

		Keyboard key = new Keyboard();
		
		key.setSize(1200,800);
		key.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		key.pack();		//packs all the keys into the frame
		key.setVisible(true);
	}

}
